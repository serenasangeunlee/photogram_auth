<div class="page-header">
  <h1>
    My Likes
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>User</th>
      </tr>

      <% @user.liked_photos.each do |photo| %>

        <dd><a href="/photos/<%= photo.id %>"><img src="<%=photo.image%>"></a></dd>
        <dd><%= photo.caption %></dd>
      <% end %>


    </table>
  </div>
</div>
